@page
@model Acme.OnlineCourses.Pages.EmploymentSupportAdmin.IndexModel
@{
    ViewData["Title"] = "Employment Support Admin";
}

<h2>Employment Support Admin</h2>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Họ và tên</th>
            <th>Ngày sinh</th>
            <th>Số điện thoại</th>
            <th>Email</th>
            <th>Địa chỉ</th>
            <th>Ngày hoàn thành khóa học</th>
            <th>Tin nhắn</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.EmploymentSupports)
    {
        <tr>
            <td>@item.FullName</td>
            <td>@item.DateOfBirth.ToString("dd/MM/yyyy")</td>
            <td>@item.PhoneNumber</td>
            <td>@item.Email</td>
            <td>@item.Address</td>
            <td>@item.CourseCompletionDate.ToString("dd/MM/yyyy")</td>
            <td>@item.Message</td>
        </tr>
    }
    </tbody>
</table> 